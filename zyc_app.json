{
  "title": "Launch MacOS apps",
  "rules": [
    {
      "description": "Launch apps by left command+left control+left option+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_gui",
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Safari.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_option",
                "left_gui",
                "left_control"
              ]
            }
          },
          "to": [
